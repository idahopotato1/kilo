{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "removeComments": true,
    "moduleResolution": "node",
    "outDir": "./dist/"
  },
  "typedocOptions": {
    "out": "docs",
    "categorizeByGroup": false,
    "entryPoint": "kilo",
    "excludeExternals": true,
    "excludeNotExported": true,
    "excludePrivate": true,
    "theme": "minimal",
    "name": "kilo",
    "hideGenerator": true
  }
}
